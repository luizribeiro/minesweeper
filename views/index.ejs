<!DOCTYPE html>
<html>
    <head>
        <title>minesweeper</title>
        <meta charset="UTF-8" />
        <link href='https://fonts.googleapis.com/css?family=Oleo+Script:700' rel='stylesheet' type='text/css' />
        <link rel="stylesheet" href="style.css" type="text/css" />
        <link rel="icon" type="image/png" href="img/favicon.png" />
    </head>
    <body>
        <h1>minesweeper</h1>
        <div id="content"></div>
        <a href="http://github.com/luizribeiro/minesweeper"><img style="position: absolute; top: 0; right: 0; border: 0;" src="img/fork.png" alt="Fork me on GitHub"></a>
        <script src="lib/jquery-1.7.2.min.js"></script>
        <script src="lib/socket.io.js"></script>
        <script src="utils.js"></script>
        <script src="resources.js"></script>
        <script src="controller.js"></script>
        <script src="model.js"></script>
        <script src="state.js"></script>
        <script src="network.js"></script>
        <script src="main.js"></script>
        <script>
        window.fbAsyncInit = function() {
          FB.init({
            appId : '<%= config.FACEBOOK_APP_ID %>',
            status : true,
            cookie : true,
            xfbml : true
          });
          launchGame();
        };
        (function(d){
          var js, id = 'facebook-jssdk'; if (d.getElementById(id)) {return;}
          js = d.createElement('script'); js.id = id; js.async = true;
          js.src = "//connect.facebook.net/en_US/all.js";
          d.getElementsByTagName('head')[0].appendChild(js);
        }(document));
        </script>
    </body>
